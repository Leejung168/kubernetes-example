{
    "kind": "Pod",
    "apiVersion": "v1",
    "metadata": {
        "name": "nginx-tst-a",
        "labels": {
            "name": "service-nginx"
         }
    },
    "spec": {
        "containers": [
            {
                "name": "nginx",
                "image": "nginx:latet",
                "ports": [
                    {
                        "containerPort": 80,
                        "hostPort": 8088
                     }
                 ]
             }
         ]
     }
}
